{
  "name": "@rocketmakers/orbit-ui-kit",
  "version": "0.0.2-alpha.6",
  "description": "Rocketmakers Orbit library of React components and styling",
  "main": "./dist/index.js",
  "style": "./dist/style.scss",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "test": "npm run build-ts && mocha --reporter spec",
    "build-ts": "tsc --pretty -p source -d",
    "build-scss": "node sass_concat.js",
    "build": "npm run build-ts && npm run build-scss",
    "start": "cd playground && webpack-dev-server --config config/webpack.config.js --env.environment=dev",
    "watch-scss": "node sass_copy.js",
    "watch-ts": "tsc -p source -d -w",
    "watch": "parallelshell -v \"npm run watch-ts\" \"npm run watch-scss\"",
    "lint": "tslint -c tslint.json 'source/**/*.ts{,x}'",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rocketmakers/orbit-ui-kit.git"
  },
  "keywords": [
    "typescript",
    "React",
    "sass"
  ],
  "author": "developers@rocketmakers.com",
  "license": "MIT",
  "peerDependencies": {
    "@types/react": "16.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-router-dom": "5.x",
    "@rocketmakers/armstrong": "0.x"
  },
  "devDependencies": {
    "@types/mocha": "5.2.7",
    "@types/node": "13.1.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "16.9.4",
    "@types/underscore": "1.9.4",
    "chokidar": "3.3.1",
    "import-resolve": "0.4.0",
    "mocha": "7.0.1",
    "parallelshell": "3.0.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "@rocketmakers/armstrong": "0.0.1-alpha.45",
    "react-hooks-testing-library": "0.6.0",
    "react-test-renderer": "16.12.0",
    "ts-loader": "6.0.4",
    "tslib": "1.10.0",
    "tslint": "5.20.0",
    "typescript": "3.7.5",
    "@types/react-router-dom": "^5.1.2",
    "autoprefixer": "^9.7.1",
    "browser-sync": "^2.26.7",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "history": "^4.10.1"
  }
}
